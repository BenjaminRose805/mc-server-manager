{
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "mc-backend",
          "sidecar": true,
          "args": true
        }
      ]
    },
    "shell:allow-open",
    "process:default"
  ]
}
